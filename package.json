{
  "name": "iletsb-records-retention-inventory",
  "version": "1.0.0",
  "description": "Illinois Law Enforcement Training & Standards Board Records Retention Inventory - Offline-first HTML application for managing records retention schedules and series items",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server . -p 8080 -o",
    "dev": "npx http-server . -p 8080 -o --cors",
    "lint": "npx eslint app.js",
    "lint:fix": "npx eslint app.js --fix",
    "test": "npx vitest",
    "test:watch": "npx vitest --watch",
    "build": "echo 'No build process needed for static HTML app'",
    "validate": "npx html-validate index.html",
    "audit": "npm audit",
    "serve": "npx serve ."
  },
  "keywords": [
    "records-management",
    "retention-schedule",
    "offline-first",
    "indexeddb",
    "accessibility",
    "responsive-design",
    "iletsb",
    "government"
  ],
  "author": "ILETSB Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iletsb/records-retention-inventory.git"
  },
  "bugs": {
    "url": "https://github.com/iletsb/records-retention-inventory/issues"
  },
  "homepage": "https://github.com/iletsb/records-retention-inventory#readme",
  "devDependencies": {
    "eslint": "^8.57.0",
    "html-validate": "^8.7.4",
    "http-server": "^14.1.1",
    "serve": "^14.2.1",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "files": [
    "index.html",
    "app.js",
    "style.css",
    "README.md",
    "test-import.json"
  ]
}
